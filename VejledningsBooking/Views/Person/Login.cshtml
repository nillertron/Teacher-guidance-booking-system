@model VejledningsBooking.ViewModel.LoginViewModel
@Html.ValidationSummary()
<form class="form-group" asp-action="Login">
    @Html.AntiForgeryToken()
    <label>User name!</label>
    <input class="form-control" type="text" asp-for="Username" placeholder="User Name" />
    <label>Password!</label>
    <input class="form-control" type="password" asp-for="Password" placeholder="Password" />
    <button class="btn btn-primary" type="submit">Login</button>
</form>
